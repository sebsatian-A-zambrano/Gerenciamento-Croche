{
  "version": 2,
  "buildCommand": "pnpm run vercel-build",
  "installCommand": "pnpm install",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/public" }
    }
  ],
  "routes": [
    { "src": "^/(.*)$", "dest": "/index.html" }
  ]
}
